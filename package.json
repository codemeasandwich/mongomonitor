{
  "name": "mongomonitor",
  "version": "0.2.0",
  "description": "mongo monitoring",
  "main": "index.js",
  "scripts": {
    "setup": "touch /data/mongod.log && chmod 777 /data/mongod.log",
    "keyfile" : "openssl rand -base64 756 > ./keyfile && chmod 400 ./keyfile",
    "mongo": "mongod --config ./mongod.conf",
    "log": "tail -f /data/mongod.log",
    "start": "node index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.5",
    "node-fetch": "^2.1.2",
    "ps-tree": "^1.1.0",
    "rxjs": "^6.1.0",
    "tail": "^1.2.3"
  }
}
